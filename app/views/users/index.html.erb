<%= render 'users/side', user: current_user %>
<%= render 'books/side', book: @book %>


<% @users.each do |user| %>
<table>
    <thead>
        <tr>
            <th>image</th>
            <th>name</th>
            <th></th>
        </tr>
    </thead>
<%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no-image.jpg" %>
<%= user.name %>
<%= link_to "Show", user_path(user.id) %>
</table>
<% end %>